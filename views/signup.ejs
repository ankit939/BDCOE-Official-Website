  <script src="/js/dependencies/angular-aria.min.js"></script>
  <script src="/js/dependencies/angular-material.min.js"></script>
  <script src="/js/dependencies/angular-messages.min.js"></script>
  <script src="/js/dependencies/angular-resource.js"></script>
  <script src="/js/dependencies/angular-route.js"></script>
  <script src="/js/dependencies/angular-toastr.js"></script>
  <script src="/js/dependencies/angular-toastr.tpls.js"></script>
  <script src="/js/dependencies/animate.js"></script>
  <script src="/js/dependencies/dialogs-default-translations.js"></script>
  <script src="/js/dependencies/dialogs.js"></script>
  <script src="/js/dependencies/jquery.BlackAndWhite.js"></script>
  <script src="/js/dependencies/jquery.flexslider-min.js"></script>
  <script src="/js/dependencies/jquery.nicescroll.min.js"></script>
  <script src="/js/dependencies/jquery.prettyPhoto.js"></script>
  <script src="/js/dependencies/members.js"></script>
  <script src="/js/dependencies/owl.carousel.js"></script>
  <script src="/js/dependencies/superfish.min.js"></script>
  <script src="/js/dependencies/ui-bootstrap-tpls-1.3.2.js"></script>
  <script src="/js/dependencies/wow.js"></script>
  <script src="/js/script.js"></script>
  <script src="/js/public/homepage/HomeMod.js"></script>
  <script src="/js/public/homepage/HomeCtrl.js"></script>
  <script src="/js/public/login/LoginMod.js"></script>
  <script src="/js/public/login/LoginCtrl.js"></script>
  <script src="/js/public/signup/SignupMod.js"></script>
  <script src="/js/public/signup/SignupCtrl.js"></script>
  <script src="/js/private/dashboard/DashMod.js"></script>
  <script src="/js/private/dashboard/DashCtrl.js"></script>
  <!--SCRIPTS END-->  
</body>
</html>



  

