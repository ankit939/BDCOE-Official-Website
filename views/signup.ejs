  <link rel="stylesheet" href="/styles/animate.css">
  <link rel="stylesheet" href="/styles/font-awesomenew.css">
  <link rel="stylesheet" href="/styles/googleapis.css">
  <link rel="stylesheet" href="/styles/hover.css">
  <link rel="stylesheet" href="/styles/importer.css">
  <link rel="stylesheet" href="/styles/sidebar-left.css">
  <!--STYLES END-->
</head>
<body ng-app="SignupMod" ng-controller="SignupCtrl">
  <!-- PRELOADER -->
<img id="preloader" src="images/preloader.gif" alt="" />
<!-- //PRELOADER -->
<div class="preloader_hide">

  <!-- PAGE -->
  <div id="page" style="min-width:900px">
  
    <!-- HEADER -->
    <header id="navbar">
      
      <!-- MENU BLOCK -->
      <div class="menu_block">
      
        <!-- CONTAINER -->
        <div class="container clearfix" style="min-width: 1050px;">
          
          <!-- LOGO -->
          <div class="logo pull-left">
            <a href="/#/" ><span class="b1">B</span><span class="b2">D</span><span class="b3">C</span><span class="b4">o</span><span class="b5">E</span></a>
          </div><!-- //LOGO -->
          
          <!-- MENU -->
          <div class="pull-right" style="position: absolute;left: 16%;right: 15%;">
            <nav class="navmenu center" style="min-width: 951px;left: 17%;right: 17%;">
              <ul>
                <li class="first scroll_btn"><a href="/#/"><span class=" glyphicon glyphicon-home" style="padding-right:3px"></span>Home</a></li>
                <li class="scroll_btn"><a href="/#/what_we_do"><span class=" glyphicon glyphicon-cog" style="padding-right:3px"></span>What We Do</a></li>
                <li class="scroll_btn"><a href="/#/portfolio"><span class=" glyphicon glyphicon-picture" style="padding-right:3px"></span>Portfolio</a></li>
                <li class="scroll_btn"><a href="/#/about_us"><span class=" glyphicon glyphicon-tree-deciduous" style="padding-right:3px"></span>About Us</a></li>
                <li class="scroll_btn"><a href="/#/news"><span class=" glyphicon glyphicon-list-alt" style="padding-right:3px"></span>News</a></li>
                <li class="scroll_btn last"><a href="/#/contact_us"><span class=" glyphicon glyphicon-phone" style="padding-right:3px"></span>Contact Us</a></li>
                <li><a href="/login" ><span class=" glyphicon glyphicon-user" style="padding-right:3px"></span>Login</a></li>
                <li><a href="/signup" ><span class=" glyphicon glyphicon-pencil" style="padding-right:3px"></span>Sign Up</a></li>
              </ul>
            </nav>
          </div><!-- //MENU -->
        </div><!-- //MENU BLOCK -->
      </div><!-- //CONTAINER -->
    </header><!-- //HEADER -->
<div id="loginbg" style="margin-left:0;margin-right:0;background-image: url(../images/white.png);">
  <div class="loginformbg">
  <form ng-submit="runSignup()" style="position: absolute;right: 27%;left: 27%;top: 19%;">
  <div class="container" style="width:99%">
  <div class="col-2">
    <label>
      Firstname
      <input ng-model="firstname" class="form-control" ng-maxlength="10"   tabindex="1" placeholder="Enter firstname..." required />
    </label>
  </div>
  <div class="col-2">
    <label>
      Lastname
      <input ng-model="lastname" class="form-control" ng-maxlength="20"  tabindex="2" placeholder="Enter lastname..." required />
    </label>
  </div>
  <div class="col-2">
    <label>
      Email
      <input  type="email" class="form-control" ng-model="email"  tabindex="3" placeholder="Enter email..." required />
    </label>
  </div>
  <div class="col-2">
    <label>
      Password
      <input type="password" class="form-control" ng-model="password" tabindex="4" placeholder="Enter password..." required />
    </label>
  </div>
  <div>
    <button type="submit" class="form-control" style="position:relative;width: 47%;left: 25%;right: 25%;top:27%">Signup</button>
    <p class="message">Already registered? <a href="/login">Login here</a></p>
  </div>
  </form> 
  </div>
</div>
<div>
      <img src="/images/corousel1.png" class="corouselimg img-responsive" style="left: 5.9%;margin-top: 80px;">
      <div class="corouseldiv carousel slide img-responsive" id="myCarousel" data-ride="carousel" style="margin-top: -26.4%;left: 9.8%;">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox" style="width:125%">
    <div class="item active">
      <img src="/images/corousel/2.jpg">
    </div>
    <div class="item">
      <img src="/images/corousel/3.jpg">
    </div>
    <div class="item">
      <img src="/images/corousel/6.jpg">
    </div>
  </div>
</div>
      </div>
  </div>
  </div>
