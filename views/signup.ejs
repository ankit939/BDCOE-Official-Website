        <!-- CONTAINER -->
        <div class="container clearfix">
          
          <!-- LOGO -->
          <div class="logo pull-left">
            <a href="/#/" ><span class="b1">B</span><span class="b2">D</span><span class="b3">C</span><span class="b4">o</span><span class="b5">E</span></a>
          </div><!-- //LOGO -->
          
          <!-- MENU -->
          <div class="pull-right" style="position: absolute;right: 12%;">
            <nav class="navmenu center">
              <ul>
                <li class="first scroll_btn"><a href="/#/">Home</a></li>
                <li class="scroll_btn"><a href="/#/what_we_do">What We Do</a></li>
                <li class="scroll_btn"><a href="/#/portfolio">Portfolio</a></li>
                <li class="scroll_btn"><a href="/#/about_us">About Us</a></li>
