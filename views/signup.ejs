<div id="loginbg" style="margin-left:0;margin-right:0;">
  <form ng-submit="runSignup()" style="position:absolute;right: 12%;top: 30%;">
  <div class="container" >
  <div class="col-2">
    <label>
      Firstname
      <input ng-model="firstname" class="form-control" ng-maxlength="10"   tabindex="1" placeholder="Enter firstname" required />
    </label>
  </div>
  <div class="col-2">
    <label>
      Lastname
      <input ng-model="lastname" class="form-control" ng-maxlength="20"  tabindex="2" placeholder="Enter lastname" required />
    </label>
  </div>
  <div class="col-2">
    <label>
      Email
      <input  type="email" class="form-control" ng-model="email"  tabindex="3" placeholder="Enter email" required />
    </label>
  </div>
  <div class="col-2">
    <label>
      Password
      <input type="password" class="form-control" ng-model="password" tabindex="4" placeholder="Enter password" required />
    </label>
  </div>
  <div class="col-2">
    <label>
      Confirm Password
      <input type="password" class="form-control" ng-model="password2" tabindex="5" placeholder="Re-enter password" required />
    </label>
  </div>
  <div>
    <button type="submit" class="form-control" style="position:relative;width:90px;left:50px">Signup</button>
    <p class="message">Already registered? <a href="/login">Login here</a></p>
  </div>
  </form> 
  </div>
</div>  
<div>
<div class="container">
      <img src="/images/corousel1.png" class="corousel2img" style="width: 43%;">
      <div class="corousel2div carousel slide" id="myCarousel" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
    <li data-target="#myCarousel" data-slide-to="3"></li>
  </ol>
  <!-- Wrapper for slides -->
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <img src="/images/corousel/2.jpg" class="imgcarousal">
    </div>

    <div class="item">
      <img src="/images/corousel/1.png" class="imgcarousal">
    </div>

    <div class="item">
      <img src="/images/corousel/3.jpg" class="imgcarousal">
    </div>

    <div class="item">
      <img src="/images/corousel/6.jpg" class="imgcarousal">
    </div>
  </div>
</div>
      </div>
      </div>
      </div>
  </div>
