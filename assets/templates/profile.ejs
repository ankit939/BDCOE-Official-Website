    <script>
              $(function() {
                $('#search').on('keyup', function() {
                  var pattern = $(this).val();
                  $('.searchable-container .items').hide();
                  $('.searchable-container .items').filter(function() {
                  return $(this).text().match(new RegExp(pattern, 'i'));
                  }).show();
                });
              });
              </script>
    <script type="text/ng-template" id="modal.html">
        <div class="modal-header">
          <h4 class="modal-title" style+"font-size: larger;"><span class="glyphicon glyphicon-pencil"></span>&nbsp;&nbsp;Edit your Profile</h4></div>
          <div class="modal-body">
          <form ng-submit="profileEdit()">
            <div style="position:relative;width:50%">
              <label class="control-label" for="course">Student No:</label>
              <input type="text" class="form-control" name="studno" id="username" ng-model="studno" placeholder="{{user.studno}}" ng-keyup="hitEnter($event)" style="width: 90%;" required>
              <label class="control-label" for="course">University Roll No:</label>
              <input type="text" class="form-control" name="username" id="username" ng-model="univno" placeholder="{{user.univno}}" ng-keyup="hitEnter($event)" style="width: 90%;" required>
              <label class="control-label" for="course">Branch:</label>
              <input type="text" class="form-control" name="branch" id="username" ng-model="branch" placeholder="{{user.branch}}" ng-keyup="hitEnter($event)" style="width: 90%;" required>
              <label class="control-label" for="course">Section:</label>
              <input type="text" class="form-control" name="section" id="username" ng-model="section" placeholder="{{user.section}}" ng-keyup="hitEnter($event)" style="width: 90%;" required>
              <label class="control-label" for="course">Year:</label>
              <input type="text" class="form-control" name="year" id="username" ng-model="year" placeholder="{{user.year}}" ng-keyup="hitEnter($event)" style="width: 90%;" required>
              <label class="control-label" for="course">Mobile No:</label>
              <input type="text" class="form-control" name="mobileno" id="username" ng-model="mobileno" placeholder="{{user.mobileno}}" ng-keyup="hitEnter($event)" style="width: 90%;" required>
              </div>
              <div style="width: 1px;height: 78%;position: absolute;top: 8%;left: 48%;right: 52%;background-color: #D3D3D3;"></div> 
                <div style="position: absolute;left: 52%;top: 12%;width: 46%;">
                  <label class="control-label" for="course" style="position:absolute;">Interested In</label>
                  <div class="row">
                    <div class="form-group">
                      <div class="searchable-container">
                        <div ng-repeat="interest in interests">
