                            <div class="info-block block-info clearfix">
                              <div class="square-box pull-left">
                                <span class="glyphicon glyphicon-tags glyphicon-lg"></span>
                              </div>
                              <div data-toggle="buttons" class="btn-group bizmoduleselect">
                                <label class="btn btn-default" style="padding:0">
                                <div class="bizcontent">
                                  <input type="checkbox" name="intlist" autocomplete="off" value="">
                                  <span class="glyphicon glyphicon-ok glyphicon-lg"></span>
                                  <h5>{{interest}}</h5>
                                </div>
                                </label>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              <button type="button" class="btn btn-default" ng-click="cancel()" style="position: relative;left: 72%;height: 53px;    width: 13%;bottom: 3.5%;">Cancel</button>
              <button type="submit" class="btn btn-primary" ng-disabled="(nameDialog.$dirty && nameDialog.$invalid) || nameDialog.$pristine" style="position: relative;left:73%;height: 53px;width: 13%;bottom: 3%;">Save</button>
            </form>
    </script>
